<h1>Delivery Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivery_types.each do |delivery_type| %>
      <tr>
        <td><%= delivery_type.name %></td>
        <td><%= link_to 'Show', delivery_type %></td>
        <td><%= link_to 'Edit', edit_delivery_type_path(delivery_type) %></td>
        <td><%= link_to 'Destroy', delivery_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Delivery Type', new_delivery_type_path %>
